wb = xlsx_package.workbook
wb.add_worksheet(name: "Visits") do |sheet|
  sheet.add_row [t("visit.device_id"), t("visit.page"), t("facility.facility"), t("visit.visit_date")]

  @visits.each do |visit|
    sheet.add_row [visit.app_user.device_id, visit.page_name, visit.facility_name, visit.visit_date]
  end
end
